{
  "extends": "tslint:latest",
  "rules": {
    "no-bitwise": false,
    "max-line-length": [
      true,
      320
    ],
    "no-var-requires": false,
    "variable-name": false,
    "no-unused-variable": false,
    "no-console": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-empty": false,
    "prefer-for-of": false
  }
}