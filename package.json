{
  "name": "EverCookie",
  "version": "1.0.18",
  "description": "Versatile storage.",
  "keywords": [
    "TypeScript",
    "Storage",
    "EverCookie"
  ],
  "homepage": "https://github.com/CrazySquirrel/EverCookie",
  "bugs": {
    "url": "https://github.com/CrazySquirrel/EverCookie/issues",
    "email": "info@crazysquirrel.ru"
  },
  "license": "MIT",
  "author": "Sergey Yastrebov <info@crazysquirrel.ru>",
  "main": "./lib/EverCookie.js",
  "module": "./lib/EverCookie.js",
  "browser": "./lib/EverCookie.js",
  "jsnext:main": "./lib/EverCookie.js",
  "types": "./lib/EverCookie.d.ts",
  "bin": {
    "AnimationFrame": "./lib/EverCookie.js"
  },
  "man": "./README.md",
  "repository": {
    "type": "git",
    "url": "git@github.com:CrazySquirrel/EverCookie.git"
  },
  "scripts": {
    "development": "NODE_ENV=development webpack --bail --config=webpack.build.config.js",
    "development:watch": "NODE_ENV=development webpack --config=webpack.build.config.js --watch",
    "production": "NODE_ENV=production webpack --bail --config=webpack.build.config.js",
    "production:watch": "NODE_ENV=production webpack --config=webpack.build.config.js --watch",
    "testing": "NODE_ENV=testing webpack --bail --config=webpack.build.config.js",
    "testing:watch": "NODE_ENV=testing webpack --config=webpack.build.config.js --watch",
    "test": "rm -rf ./doc/report/ && rm -rf ./doc/coverage/ && NODE_ENV=testing karma start karma.conf.js",
    "stats": "NODE_ENV=development webpack --bail --config=webpack.build.config.js --profile --json > ./doc/stats.json",
    "docs": "rm -rf ./doc/js/ && typedoc --mode file --target ES6 --module 'commonjs' --out ./doc/js/ ./src/ts/",
    "lint": "codeclimate analyze",
    "release": "standard-version && git push origin master --tags",
    "commit": "git-cz -a"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "eslintConfig": {
    "env": {
      "es6": true
    }
  },
  "dependencies": {
    "AnimationFrame": "^1.x.x"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.47",
    "@types/jasmine-expect": "^3.6.1",
    "@types/jasmine-matchers": "^0.2.30",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "browser-sync": "^2.15.0",
    "browser-sync-webpack-plugin": "^1.1.2",
    "clean-webpack-plugin": "^0.1.13",
    "commitizen": "^2.9.6",
    "compression": "^1.6.2",
    "compression-webpack-plugin": "^0.3.1",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^3.4.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-loader": "^0.4.4",
    "jasmine": "^2.5.2",
    "jasmine-core": "^2.5.2",
    "jasmine-expect": "^3.0.1",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-browserstack-launcher": "^1.1.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-matchers": "^3.0.1",
    "karma-opera-launcher": "^1.0.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-safari-launcher": "^1.0.0",
    "karma-source-map-support": "^1.2.0",
    "karma-webpack": "^1.8.0",
    "standard-version": "^4.0.0",
    "string-replace-webpack-plugin": "https://github.com/CrazySquirrel/string-replace-webpack-plugin.git",
    "ts-loader": "^0.8.2",
    "tslint": "^3.15.1",
    "typedoc": "^0.5.1",
    "typescript": "^2.0.9",
    "webpack": "^1.13.3",
    "webpack-notifier": "^1.4.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
